{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".(jsx|js|ts|tsx)$"
  },
  "sourceType": "module",
  "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
  }
}